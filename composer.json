{
    "name": "alexrili/vephar",
    "type": "library",
    "description": "Small abstraction of api requests and transform responses in collections",
    "keywords": [
        "alexrili",
        "vephar"
    ],
    "homepage": "https://github.com/alexrili/vephar",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Ribeiro",
            "email": "alexrili@gmail.com",
            "homepage": "https://github.com/alexrili/vephar",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "illuminate/support": "^6.16",
        "ext-json": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Hell\\Vephar\\": "src"
        },
        "files": [
            "./src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Hell\\Vephar\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
